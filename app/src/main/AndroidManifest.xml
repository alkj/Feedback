<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="dk.spinoff.apps.feedback_app">

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name="dk.spinoff.apps.feedback_app.aktiviteter.StartSkaerm_akt"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="dk.spinoff.apps.feedback_app.aktiviteter.Login_akt"
            android:noHistory="true"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name="dk.spinoff.apps.feedback_app.aktiviteter.OpretBruger_akt"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name="dk.spinoff.apps.feedback_app.aktiviteter.Overholdt_Giv_Feedback_akt"
            android:noHistory="true"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name="dk.spinoff.apps.feedback_app.aktiviteter.Navigation_akt"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name="dk.spinoff.apps.feedback_app.aktiviteter.TakForFeedback"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name="dk.spinoff.apps.feedback_app.aktiviteter.AfholdtMeode_akt"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name="dk.spinoff.apps.feedback_app.aktiviteter.IkkeAfholdtMoede_akt"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name="dk.spinoff.apps.feedback_app.aktiviteter.beforeGivFeedback_akt"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name="dk.spinoff.apps.feedback_app.aktiviteter.AfholdtMoedeInfo_akt"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name="dk.spinoff.apps.feedback_app.aktiviteter.ikke_afholdt_moede_rediger_akt"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />


        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>
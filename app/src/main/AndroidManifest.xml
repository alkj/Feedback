<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.feedback_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".aktiviteter.StartSkaerm_akt"
            android:windowSoftInputMode="adjustPan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".aktiviteter.Login_akt"
            android:windowSoftInputMode="adjustPan"/>
        <activity android:name=".aktiviteter.OpretBruger_akt"
            android:windowSoftInputMode="adjustPan"/>
        <activity android:name=".aktiviteter.GivFeedback_akt"
            android:windowSoftInputMode="adjustPan"/>
        <activity android:name=".aktiviteter.Navigation_akt"
            android:windowSoftInputMode="adjustPan"/>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />


    </application>

</manifest>
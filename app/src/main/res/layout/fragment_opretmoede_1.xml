<?xml version="1.0" encoding="UTF-8"?>

<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorMørkegrå"
    tools:context=".fragmenter.OpretMoede_1_frg">


    <LinearLayout
        android:id="@+id/john"
        android:layout_width="275dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:background="#0AFFFFFF"
        android:orientation="vertical"
        android:padding="10dp"
        android:paddingStart="10dp"
        android:paddingLeft="10dp"
        android:paddingTop="10dp"
        android:paddingEnd="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9">

        <TextView
            android:id="@+id/textView1h0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/open_sans_semibold"
            android:paddingLeft="4dp"
            android:text="Mødeoplysninger"
            android:textColor="@color/colorHvid"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="@+id/dato"
            app:layout_constraintTop_toBottomOf="@+id/textView9" />

        <EditText
            android:id="@+id/mødeNavn"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginRight="34dp"
            android:layout_marginBottom="10dp"
            android:allowUndo="true"
            android:autofillHints="DBDBDB"
            android:clickable="false"
            android:drawablePadding="10dp"
            android:ems="10"
            android:fadingEdge="horizontal|vertical"
            android:fontFamily="@font/open_sans_semibold"
            android:hint="Navn"
            android:inputType="text"
            android:textAppearance="@style/TextAppearance.AppCompat"
            android:textColor="@color/colorMEGETLysGrå"
            android:textColorHint="@color/colorHvid"
            app:backgroundTint="@color/colorLysegrå"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/virk_id_editTxt" />

        <EditText
            android:id="@+id/mødeSted"
            android:layout_width="250dp"
            android:layout_height="match_parent"
            android:layout_marginRight="34dp"
            android:layout_marginBottom="10dp"
            android:allowUndo="true"
            android:autofillHints="DBDBDB"
            android:clickable="false"
            android:drawablePadding="10dp"
            android:ems="10"
            android:fadingEdge="horizontal|vertical"
            android:fontFamily="@font/open_sans_semibold"
            android:hint="Sted"
            android:inputType="text"
            android:textAppearance="@style/TextAppearance.AppCompat"
            android:textColor="@color/colorMEGETLysGrå"
            android:textColorHint="@color/colorHvid"
            app:backgroundTint="@color/colorLysegrå"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/virk_id_editTxt" />

        <EditText
            android:id="@+id/mødeFormål"
            android:layout_width="250dp"
            android:layout_height="match_parent"
            android:layout_marginRight="34dp"
            android:layout_marginBottom="10dp"
            android:allowUndo="true"
            android:fontFamily="@font/open_sans_semibold"
            android:hint="Formål"
            android:inputType="textMultiLine"
            android:maxLines="2"
            android:textAppearance="@style/TextAppearance.AppCompat"
            android:textColor="@color/colorMEGETLysGrå"
            android:textColorHint="@color/colorHvid"
            app:backgroundTint="@color/colorLysegrå" />
        ;android:autofillHints="DBDBDB"android:clickable="false"android:drawablePadding="10dp"android:ems="10"android:fadingEdge="horizontal|vertical"android:fontFamily="@font/open_sans_semibold"android:hint="Formål"android:inputType="textMultiLine"android:textAppearance="@style/TextAppearance.AppCompat"android:textColor="@color/colorMEGETLysGrå"android:textColorHint="@color/colorHvid"app:backgroundTint="@color/colorLysegrå"app:layout_constraintEnd_toEndOf="parent"app:layout_constraintHorizontal_bias="0.0"app:layout_constraintStart_toStartOf="parent"app:layout_constraintTop_toBottomOf="@+id/virk_id_editTxt"
        />
    </LinearLayout>

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="8dp"
        android:fontFamily="@font/open_sans_bold"
        android:text="Opret Møde"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="@color/colorHvid"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:fontFamily="@font/open_sans_semibold"
        android:text="1 af 2"
        android:textAlignment="textStart"
        android:textColor="@color/colorHvid"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <Button
        android:id="@+id/videreBTN"
        android:layout_width="173dp"
        android:layout_height="54dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="8dp"
        android:autofillHints="DBDBDB"
        style="@style/Knap.Lysegrå"
        android:fontFamily="sans-serif"
        android:text="Videre"
        android:textColor="#FBF7FF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.486"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/john" />

</android.support.constraint.ConstraintLayout>
<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".uddyb_bedommelse"
    tools:layout_editor_absoluteY="25dp">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="318dp"
        android:layout_height="71dp"
        android:fontFamily="@font/open_sans_bold"
        android:text="Hvad syntes du om PowerPoint præsentationen?"
        android:textAlignment="center"
        android:textColor="@color/colorMørkegrå"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="63dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="83dp"
        android:layout_height="93dp"
        app:srcCompat="@mipmap/ic_launcher"
        tools:layout_editor_absoluteX="139dp"
        tools:layout_editor_absoluteY="230dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hvorfor?"
        android:textColor="@color/colorGrøn"
        android:textSize="45sp"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="169dp" />

    <EditText
        android:id="@+id/editText3"
        android:layout_width="214dp"
        android:layout_height="137dp"
        android:background="@color/colorMEGETLysGrå"
        android:ems="10"
        android:hint="Uddyb her"
        android:inputType="textPersonName"
        android:selectAllOnFocus="false"
        android:singleLine="false"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="330dp" />

    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="214dp"
        android:layout_height="62dp"
        android:orientation="horizontal"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="480dp">

        <Button
            android:id="@+id/button4"
            android:layout_width="65sp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@color/colorGrøn"
            android:text="Button"
            android:textColor="@android:color/white" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="10dp"
            android:layout_height="wrap_content" />

        <Button
            android:id="@+id/button3"
            android:layout_width="65sp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@color/colorMørkegrå"
            android:text="Button"
            android:textColor="@android:color/white" />
    </LinearLayout>

</android.support.constraint.ConstraintLayout>
<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorMørkegrå"
    tools:context=".aktiviteter.Afholdt_MoedeInformationer_akt">

    <ScrollView
        style="@android:style/Widget.Material.ScrollView"
        android:layout_width="368dp"
        android:layout_height="328dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:clipToPadding="true"
        android:fadeScrollbars="false"
        android:fillViewport="true"
        android:padding="5dp"
        android:scrollbarAlwaysDrawHorizontalTrack="true"
        android:scrollbarFadeDuration="0"
        android:scrollbarSize="8dp"
        app:layout_constraintBottom_toTopOf="@+id/ikkeAfholdtStart_knap"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.481"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv1010n">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingTop="3dp"
            android:paddingBottom="3dp">

            <TextView
                android:id="@+id/tvasd"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Mødenavn"
                android:textColor="@color/colorHvid"
                android:textSize="20sp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="216dp"
                tools:layout_editor_absoluteY="306dp" />

            <TextView
                android:id="@+id/tvMødeInfoMødeNavn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="3dp"
                android:paddingBottom="20dp"
                android:text="x"
                android:textColor="@color/colorHvid" />

            <TextView
                android:id="@+id/tv000"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Møde formål"
                android:textColor="@color/colorHvid"
                android:textSize="20sp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="83dp"
                tools:layout_editor_absoluteY="198dp" />

            <TextView
                android:id="@+id/tvMødeInfoMødeFormaal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="3dp"
                android:paddingBottom="20dp"
                android:text="x"
                android:textColor="@color/colorHvid" />

            <TextView
                android:id="@+id/tv0000"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Mødested"
                android:textColor="@color/colorHvid"
                android:textSize="20sp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="83dp"
                tools:layout_editor_absoluteY="198dp" />

            <TextView
                android:id="@+id/tvMødeInfoMødeSted"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="3dp"
                android:paddingBottom="20dp"
                android:text="x"
                android:textColor="@color/colorHvid" />

            <TextView
                android:id="@+id/tv00030"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Møde ID"
                android:textColor="@color/colorHvid"
                android:textSize="20sp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="83dp"
                tools:layout_editor_absoluteY="198dp" />

            <TextView
                android:id="@+id/tvMødeInfoMødeID"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="3dp"
                android:paddingBottom="20dp"
                android:text="x"
                android:textColor="@color/colorHvid" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv00000"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="left"
                    android:text="Planlagt Starttid"
                    android:textColor="@color/colorHvid"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tv000000"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="left"
                    android:text="Planlagt Sluttid"
                    android:textColor="@color/colorHvid"
                    android:textSize="20sp"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:paddingBottom="20dp">

                <TextView
                    android:id="@+id/tvMødeInfoPlanlagtStarttid"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="left"
                    android:text="xx"
                    android:textColor="@color/colorHvid" />


                <TextView
                    android:id="@+id/tvMødeinfoPlanlagtSluttid"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="left"
                    android:text="xx"
                    android:textColor="@color/colorHvid" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv00f000"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="left"
                    android:text="Aktuel Starttid"
                    android:textColor="@color/colorHvid"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tv00001200"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="left"
                    android:text="Aktuel Sluttid"
                    android:textColor="@color/colorHvid"
                    android:textSize="20sp"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:paddingBottom="20dp">

                <TextView
                    android:id="@+id/tvMødeInfoAktuelStarttid"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="left"
                    android:text="xx"
                    android:textColor="@color/colorHvid" />


                <TextView
                    android:id="@+id/tvMødeInfoAktuelSluttid"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="left"
                    android:text="x"
                    android:textColor="@color/colorHvid" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv000000000"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="left"
                    android:text="Inviterede"
                    android:textColor="@color/colorHvid"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tv0000000"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="left"
                    android:text="Fremmødte"
                    android:textColor="@color/colorHvid"
                    android:textSize="20sp"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvMødeInfoInviterede"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="left"
                    android:text="Indsæt antal inviterede"
                    android:textColor="@color/colorHvid" />

                <TextView
                    android:id="@+id/tvMødeInfoFremmødte"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="left"
                    android:text="Indsæt antal deltagere"
                    android:textColor="@color/colorHvid" />

            </LinearLayout>

        </LinearLayout>
    </ScrollView>

    <TextView
        android:id="@+id/tv1010n"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:text="Mødeoplysninger"
        android:textAllCaps="true"
        android:textColor="@color/colorHvid"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>
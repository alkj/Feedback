<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AendreOplysninger"
    android:background="#353535"
    tools:layout_editor_absoluteY="25dp">

    <TextView
        android:id="@+id/aendre_oplysninger_overskrift"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:text="Personoplysninger"
        android:textAllCaps="true"
        android:textColor="@android:color/background_light"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.07" />

    <TextView
        android:id="@+id/aendre_oplysninger_overskrift2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:text="@string/indtast_moede_oplysninger"
        android:textAllCaps="true"
        android:textColor="@android:color/background_light"
        android:textSize="17sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.172" />

    <EditText
        android:id="@+id/aendre_fornavn_editTxt"
        android:layout_width="264dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="28dp"
        android:allowUndo="true"
        android:autofillHints="DBDBDB"
        android:clickable="false"
        android:drawableLeft="@drawable/icon_name"
        android:drawablePadding="10dp"
        android:ems="10"
        android:fadingEdge="horizontal|vertical"
        android:hint="Fornavn"
        android:inputType="textPersonName"
        android:text="Johnny"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:textColor="@color/colorMEGETLysGrå"
        android:textColorHint="@color/colorHvid"
        app:backgroundTint="@color/colorLysegrå"
        app:layout_constraintBottom_toTopOf="@+id/aendre_efternavn_editTxt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/aendre_efternavn_editTxt"
        android:layout_width="264dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="28dp"
        android:allowUndo="true"
        android:autofillHints="DBDBDB"
        android:clickable="false"
        android:drawableLeft="@drawable/icon_name"
        android:drawablePadding="10dp"
        android:ems="10"
        android:fadingEdge="horizontal|vertical"
        android:hint="Efternavn"
        android:inputType="textPersonName"
        android:text="Hansen"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:textColor="@color/colorMEGETLysGrå"
        android:textColorHint="@color/colorHvid"
        app:backgroundTint="@color/colorLysegrå"
        app:layout_constraintBottom_toTopOf="@+id/aendre_tlf_editTxt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/aendre_tlf_editTxt"
        android:layout_width="264dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="28dp"
        android:allowUndo="true"
        android:autofillHints="DBDBDB"
        android:clickable="false"
        android:drawableLeft="@drawable/icon_phone"
        android:drawablePadding="10dp"
        android:ems="10"
        android:fadingEdge="horizontal|vertical"
        android:hint="Telefon"
        android:inputType="number"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:textColor="@color/colorMEGETLysGrå"
        android:textColorHint="@color/colorHvid"
        app:backgroundTint="@color/colorLysegrå"
        app:layout_constraintBottom_toTopOf="@+id/aendre_virksomhedsid_textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:text="88888888" />

    <TextView
        android:id="@+id/aendre_virksomhedsid_textView"
        android:layout_width="264dp"
        android:layout_height="39dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="44dp"
        android:allowUndo="true"
        android:autofillHints="DBDBDB"
        android:clickable="false"
        android:drawableLeft="@drawable/icon_virk_id"
        android:drawablePadding="10dp"
        android:ems="10"
        android:fadingEdge="horizontal|vertical"
        android:hint="Telefon"
        android:inputType="number"
        android:text="Virksomheds ID: esdijkfbdsj"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:textColor="@color/colorMEGETLysGrå"
        android:textColorHint="@color/colorHvid"
        app:backgroundTint="@color/colorLysegrå"
        app:layout_constraintBottom_toTopOf="@+id/aendre_gem_btn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/aendre_gem_btn"
        android:layout_width="173dp"
        android:layout_height="54dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:autofillHints="DBDBDB"
        android:background="@drawable/knap_groen"
        android:fontFamily="sans-serif"
        android:text="gem "
        android:textColor="#FBF7FF"
        android:textSize="20sp"
        android:textStyle="bold"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.909" />

</android.support.constraint.ConstraintLayout>
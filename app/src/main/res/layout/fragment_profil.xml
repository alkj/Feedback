<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragmenter.Profil_frg"
    tools:layout_editor_absoluteY="25dp">


    <LinearLayout
        android:id="@+id/Profil_textViewContainer_layout"
        android:layout_width="275dp"
        android:layout_height="173dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="75dp"
        android:layout_marginEnd="68dp"
        android:orientation="vertical"
        android:padding="10dp"
        android:paddingStart="10dp"
        android:paddingLeft="10dp"
        android:paddingTop="10dp"
        android:paddingEnd="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/profil_fornavn_textView"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:allowUndo="true"
            android:autofillHints="DBDBDB"
            android:clickable="false"
            android:drawableLeft="@drawable/icon_name"
            android:drawablePadding="10dp"
            android:ems="10"
            android:fadingEdge="horizontal|vertical"
            android:text="Johnny"
            android:textAppearance="@style/TextAppearance.AppCompat"
            android:textColor="@color/colorMEGETLysGrå"
            android:textColorHint="@color/colorHvid"
            android:textSize="14dp"
            app:backgroundTint="@color/colorLysegrå"
            tools:layout_editor_absoluteX="60dp"
            tools:layout_editor_absoluteY="110dp" />

        <TextView
            android:id="@+id/profil_efternavn_textView"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:allowUndo="true"
            android:autofillHints="DBDBDB"
            android:clickable="false"
            android:drawableLeft="@drawable/icon_name"
            android:drawablePadding="10dp"
            android:ems="10"
            android:fadingEdge="horizontal|vertical"
            android:text="Hansen"
            android:textAppearance="@style/TextAppearance.AppCompat"
            android:textColor="@color/colorMEGETLysGrå"
            android:textColorHint="@color/colorHvid"
            android:textSize="14dp"
            app:backgroundTint="@color/colorLysegrå"
            tools:layout_editor_absoluteX="60dp"
            tools:layout_editor_absoluteY="154dp" />


        <TextView
            android:id="@+id/profil_tlf_textView"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:allowUndo="true"
            android:autofillHints="DBDBDB"
            android:clickable="false"
            android:drawableLeft="@drawable/icon_phone"
            android:drawablePadding="10dp"
            android:ems="10"
            android:fadingEdge="horizontal|vertical"
            android:hint="Telefon"
            android:inputType="number"
            android:textAppearance="@style/TextAppearance.AppCompat"
            android:textColor="@color/colorMEGETLysGrå"
            android:textColorHint="@color/colorHvid"
            android:textSize="14dp"
            app:backgroundTint="@color/colorLysegrå"
            tools:layout_editor_absoluteX="60dp"
            tools:layout_editor_absoluteY="198dp"
            tools:text="88888888" />

        <TextView
            android:id="@+id/profil_virksomhedsid_textView"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:allowUndo="true"
            android:autofillHints="DBDBDB"
            android:clickable="false"
            android:drawableLeft="@drawable/icon_virk_id"
            android:drawablePadding="10dp"
            android:ems="10"
            android:fadingEdge="horizontal|vertical"
            android:text="Virksomheds ID"
            android:textAppearance="@style/TextAppearance.AppCompat"
            android:textColor="@color/colorMEGETLysGrå"
            android:textColorHint="@color/colorHvid"
            android:textSize="14dp"
            app:backgroundTint="@color/colorLysegrå"
            tools:layout_editor_absoluteX="60dp"
            tools:layout_editor_absoluteY="243dp" />


    </LinearLayout>

    <LinearLayout
        android:id="@+id/Profil_buttons_layout"
        android:layout_width="275dp"
        android:layout_height="183dp"
        android:layout_marginStart="68dp"
        android:layout_marginEnd="68dp"
        android:layout_marginBottom="25dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <Button
            android:id="@+id/profil_rediger_button"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_gravity="center"
            android:layout_margin="10dp"
            android:background="@color/colorMEGETLysGrå"
            android:gravity="center"
            android:text="Rediger profil"
            tools:layout_editor_absoluteX="80dp"
            tools:layout_editor_absoluteY="504dp" />

        <Button
            android:id="@+id/profil_skiftPassword_button"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_gravity="center"
            android:layout_margin="10dp"
            android:background="@color/colorMEGETLysGrå"
            android:gravity="center"
            android:text="Skift Password"
            tools:layout_editor_absoluteX="80dp"
            tools:layout_editor_absoluteY="562dp" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="10dp" />

        <Button
            android:id="@+id/profil_logUd_button"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_gravity="center"
            android:layout_margin="10dp"
            android:background="#FC6363"
            android:gravity="center"
            android:text="Log ud"
            tools:layout_editor_absoluteX="80dp"
            tools:layout_editor_absoluteY="562dp" />


    </LinearLayout>


</android.support.constraint.ConstraintLayout>
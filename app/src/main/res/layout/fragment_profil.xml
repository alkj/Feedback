<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragmenter.Profil_frg"
    tools:layout_editor_absoluteY="25dp">


    <EditText
        android:id="@+id/profil_fornavn_editTxt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="60dp"
        android:layout_marginBottom="12dp"
        android:allowUndo="true"
        android:autofillHints="DBDBDB"
        android:clickable="false"
        android:drawableLeft="@drawable/icon_name"
        android:drawablePadding="10dp"
        android:ems="10"
        android:fadingEdge="horizontal|vertical"
        android:hint="Fornavn"
        android:inputType="textPersonName"
        android:text="Johnny"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:textColor="@color/colorMEGETLysGrå"
        android:textColorHint="@color/colorHvid"
        app:backgroundTint="@color/colorLysegrå"
        app:layout_constraintBottom_toTopOf="@+id/profil_efternavn_editTxt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/profil_efternavn_editTxt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginEnd="60dp"
        android:layout_marginBottom="12dp"
        android:allowUndo="true"
        android:autofillHints="DBDBDB"
        android:clickable="false"
        android:drawableLeft="@drawable/icon_name"
        android:drawablePadding="10dp"
        android:ems="10"
        android:fadingEdge="horizontal|vertical"
        android:hint="Efternavn"
        android:inputType="textPersonName"
        android:text="Hansen"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:textColor="@color/colorMEGETLysGrå"
        android:textColorHint="@color/colorHvid"
        app:backgroundTint="@color/colorLysegrå"
        app:layout_constraintBottom_toTopOf="@+id/profil_tlf_editTxt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profil_fornavn_editTxt" />

    <EditText
        android:id="@+id/profil_tlf_editTxt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginEnd="60dp"
        android:layout_marginBottom="20dp"
        android:allowUndo="true"
        android:autofillHints="DBDBDB"
        android:clickable="false"
        android:drawableLeft="@drawable/icon_phone"
        android:drawablePadding="10dp"
        android:ems="10"
        android:fadingEdge="horizontal|vertical"
        android:hint="Telefon"
        android:inputType="number"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:textColor="@color/colorMEGETLysGrå"
        android:textColorHint="@color/colorHvid"
        app:backgroundTint="@color/colorLysegrå"
        app:layout_constraintBottom_toTopOf="@+id/profil_virksomhedsid_textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profil_efternavn_editTxt"
        tools:text="88888888" />

    <TextView
        android:id="@+id/profil_virksomhedsid_textView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="60dp"
        android:layout_marginEnd="60dp"
        android:layout_marginBottom="128dp"
        android:allowUndo="true"
        android:autofillHints="DBDBDB"
        android:clickable="false"
        android:drawableLeft="@drawable/icon_virk_id"
        android:drawablePadding="10dp"
        android:ems="10"
        android:fadingEdge="horizontal|vertical"
        android:hint="Telefon"
        android:inputType="number"
        android:text="Virksomheds ID: esdijkfbdsj"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:textColor="@color/colorMEGETLysGrå"
        android:textColorHint="@color/colorHvid"
        app:backgroundTint="@color/colorLysegrå"
        app:layout_constraintBottom_toTopOf="@+id/profil_skiftPassword_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profil_tlf_editTxt" />


    <Button
        android:id="@+id/profil_skiftPassword_button"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginStart="56dp"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="20dp"
        android:gravity="center"
        android:text="Skift Password"
        app:layout_constraintBottom_toTopOf="@+id/profil_logUd_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profil_virksomhedsid_textView" />


    <Button
        android:id="@+id/profil_gem_button"
        android:layout_width="272dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="12dp"
        android:gravity="center"
        android:text="Gem Ændringer"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profil_virksomhedsid_textView" />

    <Button
        android:id="@+id/profil_logUd_button"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginStart="60dp"
        android:layout_marginEnd="60dp"
        android:layout_marginBottom="56dp"
        android:gravity="center"
        android:text="Log ud"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profil_skiftPassword_button" />


</android.support.constraint.ConstraintLayout>
<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorMørkegrå"
    tools:context=".aktiviteter.IkkeAfholdtMoede_akt">

    <TextView
        android:id="@+id/tv1010n"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:text="Mødeoplysninger"
        android:textAllCaps="true"
        android:textColor="@color/colorHvid"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="30dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv1010n">

        <TextView
            android:id="@+id/tv00030"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Møde ID"
            android:textAlignment="center"
            android:textColor="@color/colorHvid"
            android:textSize="20sp"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="17dp"
            tools:layout_editor_absoluteY="5dp" />

        <com.romainpiel.shimmer.ShimmerTextView
            android:id="@+id/tvMødeID"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="3dp"
            android:paddingBottom="10dp"
            android:text="x"
            android:textAlignment="center"
            android:textColor="@color/colorMegetGlad"
            android:textSize="30sp"
            android:textStyle="bold"
            app:reflectionColor="@color/shimmerFarveGrøn"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="110dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp"
        android:orientation="vertical"
        android:paddingTop="3dp"
        android:paddingBottom="3dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3">

        <TextView
            android:id="@+id/tvasd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Mødenavn"
            android:textColor="@color/colorHvid"
            android:textSize="16sp"
            tools:layout_editor_absoluteX="216dp"
            tools:layout_editor_absoluteY="306dp" />

        <TextView
            android:id="@+id/tvMødeNavn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="3dp"
            android:paddingBottom="20dp"
            android:text="x"
            android:textColor="@color/colorHvid"
            android:textSize="16sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/tv000"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Møde formål"
            android:textColor="@color/colorHvid"
            android:textSize="16sp"
            tools:layout_editor_absoluteX="83dp"
            tools:layout_editor_absoluteY="198dp" />

        <TextView
            android:id="@+id/tvMødeFormaal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="3dp"
            android:paddingBottom="20dp"
            android:text="x"
            android:textColor="@color/colorHvid"
            android:textSize="16sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/tv0000"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Mødested"
            android:textColor="@color/colorHvid"
            android:textSize="16sp"
            tools:layout_editor_absoluteX="83dp"
            tools:layout_editor_absoluteY="198dp" />

        <TextView
            android:id="@+id/tvMødeSted"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="3dp"
            android:paddingBottom="20dp"
            android:text="x"
            android:textColor="@color/colorHvid"
            android:textSize="16sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/tv00033"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Møde Dato"
            android:textColor="@color/colorHvid"
            android:textSize="16sp"
            tools:layout_editor_absoluteX="83dp"
            tools:layout_editor_absoluteY="198dp" />

        <TextView
            android:id="@+id/tvMødeDato"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="3dp"
            android:paddingBottom="20dp"
            android:text="x"
            android:textColor="@color/colorHvid"
            android:textSize="16sp"
            android:textStyle="bold" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv00000"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left"
                android:text="Planlagt Starttid"
                android:textColor="@color/colorHvid"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/tv000000"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left"
                android:text="Planlagt Sluttid"
                android:textColor="@color/colorHvid"
                android:textSize="16sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:paddingBottom="20dp">

            <TextView
                android:id="@+id/tvPlanlagtStarttid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left"
                android:text="xx"
                android:textColor="@color/colorHvid"
                android:textSize="16sp"
                android:textStyle="bold" />


            <TextView
                android:id="@+id/tvPlanlagtSluttid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left"
                android:text="xx"
                android:textColor="@color/colorHvid"
                android:textSize="16sp"
                android:textStyle="bold" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv000000000"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left"
                android:text="Inviterede"
                android:textColor="@color/colorHvid"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/tv0000000"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left"
                android:text="Fremmødte"
                android:textColor="@color/colorHvid"
                android:textSize="16sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tvInviterede"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left"
                android:text="x"
                android:textColor="@color/colorHvid"
                android:textSize="16sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tvFremmødte"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="left"
                android:text="x"
                android:textColor="@color/colorHvid"
                android:textSize="16sp"
                android:textStyle="bold" />

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout6"
        app:layout_constraintVertical_bias="1.0">

        <Button
            android:id="@+id/button"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:background="@drawable/knap_groen_tryk_effekt"
            android:fontFamily="@font/open_sans_bold"
            android:textColor="@color/colorHvid"
            android:text="Rediger"
            android:textSize="18sp" />

        <Button
            android:id="@+id/ikkeAfholdtStart_knap"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:autofillHints="DBDBDB"
            android:background="@drawable/knap_groen_tryk_effekt"
            android:fontFamily="@font/open_sans_bold"
            android:textColor="@color/colorHvid"
            android:text="Start møde"
            android:textSize="18sp" />

        <Button
            android:id="@+id/button2"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:background="@drawable/knap_groen_tryk_effekt"
            android:fontFamily="@font/open_sans_bold"
            android:textColor="@color/colorHvid"
            android:text="Inviter"
            android:textSize="18sp" />
    </LinearLayout>

    <android.support.v4.app.FragmentTabHost
        android:id="@+id/containerMødeStart"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

</android.support.constraint.ConstraintLayout>
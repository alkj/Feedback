<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#353535"
    tools:context=".StartScreenActivity"
    tools:layout_editor_absoluteY="25dp">

    <Button
        android:id="@+id/GivFeedbackBtn"
        android:layout_width="205dp"
        android:layout_height="54dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:autofillHints="DBDBDB"
        android:background="@drawable/knap_groen"
        android:fontFamily="@font/open_sans_bold"
        android:text="giv feedback"
        android:textColor="@color/colorHvid"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/IndtastMødeIDStartscreen"
        app:layout_constraintVertical_bias="0.218"
        tools:layout_editor_absoluteX="98dp" />

    <EditText
        android:id="@+id/IndtastMødeIDStartscreen"
        android:layout_width="244dp"
        android:layout_height="51dp"
        android:layout_marginTop="64dp"
        android:allowUndo="true"
        android:autofillHints="DBDBDB"
        android:background="#DBDBDB"
        android:ems="10"
        android:fontFamily="@font/open_sans_semibold"
        android:hint="Indtast møde-ID her..."
        android:inputType="textPersonName"
        android:paddingStart="8dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <Button
        android:id="@+id/LoginButtonStartscreen"
        android:layout_width="103dp"
        android:layout_height="37dp"
        android:background="@drawable/knap_lysegraa"
        android:text="Login"
        android:textColor="#FBF7FF"
        android:textSize="18dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="188dp"
        android:layout_height="92dp"
        android:layout_marginTop="32dp"
        android:contentDescription="Spinoff Logo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:srcCompat="@drawable/spinoff_logo" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="116dp"
        android:fontFamily="@font/open_sans_bold"
        android:text="Feedback til Møder"
        android:textAllCaps="false"
        android:textColor="@color/colorMEGETLysGrå"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>
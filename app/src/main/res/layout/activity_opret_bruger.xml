<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#353535"
    tools:context=".aktiviteter.OpretBruger_akt"
    tools:layout_editor_absoluteY="25dp">

    <EditText
        android:id="@+id/opretbruger_mail_editTxt"
        android:layout_width="192dp"
        android:layout_height="30dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="8dp"
        android:allowUndo="true"
        android:autofillHints="DBDBDB"
        android:background="#DBDBDB"
        android:clickable="false"
        android:ems="10"
        android:fadingEdge="horizontal|vertical"
        android:hint="E-mail"
        android:inputType="textPersonName"
        android:textAlignment="viewStart"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/virk_id_editTxt" />

    <EditText
        android:id="@+id/oprebruger_tlf_editTxt"
        android:layout_width="120dp"
        android:layout_height="28dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:allowUndo="true"
        android:autofillHints="DBDBDB"
        android:background="#DBDBDB"
        android:clickable="false"
        android:ems="10"
        android:fadingEdge="horizontal|vertical"
        android:hint="Telefon nr."
        android:inputType="textPersonName"
        android:textAlignment="viewStart"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/opretbruger_fornavn_editTxt"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/opretbruger_fornavn_editTxt"
        app:layout_constraintTop_toBottomOf="@+id/opretbruger_fornavn_editTxt" />

    <EditText
        android:id="@+id/opretbruger_fornavn_editTxt"
        android:layout_width="120dp"
        android:layout_height="28dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="8dp"
        android:allowUndo="true"
        android:autofillHints="DBDBDB"
        android:background="#DBDBDB"
        android:clickable="false"
        android:ems="10"
        android:fadingEdge="horizontal|vertical"
        android:hint="Fornavn"
        android:inputType="textPersonName"
        android:textAlignment="viewStart"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/opretbruger_efternavn_editTxt"
        app:layout_constraintHorizontal_bias="0.696"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/opretbruger_txtView2" />

    <EditText
        android:id="@+id/opretbruger_password2_editTxt"
        android:layout_width="190dp"
        android:layout_height="29dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="8dp"
        android:allowUndo="true"
        android:autofillHints="DBDBDB"
        android:background="#DBDBDB"
        android:clickable="false"
        android:ems="10"
        android:fadingEdge="horizontal|vertical"
        android:hint="Password igen"
        android:inputType="textPersonName"
        android:textAlignment="viewStart"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/opretbruger_password_editTxt" />

    <EditText
        android:id="@+id/opretbruger_efternavn_editTxt"
        android:layout_width="120dp"
        android:layout_height="28dp"
        android:layout_marginEnd="60dp"
        android:allowUndo="true"
        android:autofillHints="DBDBDB"
        android:background="#DBDBDB"
        android:clickable="false"
        android:ems="10"
        android:fadingEdge="horizontal|vertical"
        android:hint="Efternavn"
        android:inputType="textPersonName"
        android:textAlignment="viewStart"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/opretbruger_fornavn_editTxt" />

    <EditText
        android:id="@+id/virk_id_editTxt"
        android:layout_width="120dp"
        android:layout_height="28dp"
        android:layout_marginTop="16dp"
        android:allowUndo="true"
        android:autofillHints="DBDBDB"
        android:background="#DBDBDB"
        android:clickable="false"
        android:ems="10"
        android:fadingEdge="horizontal|vertical"
        android:hint="Virksomheds ID"
        android:inputType="textPersonName"
        android:textAlignment="textStart"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/opretbruger_efternavn_editTxt"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/opretbruger_efternavn_editTxt"
        app:layout_constraintTop_toBottomOf="@+id/opretbruger_efternavn_editTxt" />

    <EditText
        android:id="@+id/opretbruger_password_editTxt"
        android:layout_width="189dp"
        android:layout_height="29dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="8dp"
        android:allowUndo="true"
        android:autofillHints="DBDBDB"
        android:background="#DBDBDB"
        android:clickable="false"
        android:ems="10"
        android:fadingEdge="horizontal|vertical"
        android:hint="Password"
        android:inputType="textPersonName"
        android:textAlignment="viewStart"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.486"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/opretbruger_mail_editTxt" />

    <Button
        android:id="@+id/opretbruger_opret_btn"
        android:layout_width="149dp"
        android:layout_height="52dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:autofillHints="DBDBDB"
        android:background="@drawable/knap_groen"

        android:fontFamily="@font/open_sans_bold"
        android:text="Opret bruger"
        android:textColor="#FBF7FF"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/opretbruger_password2_editTxt" />

    <Button
        android:id="@+id/opretbruger_tilbage_btn"
        android:layout_width="86dp"
        android:layout_height="41dp"
        android:background="#828282"
        android:text="Annuller"
        android:textColor="#FBF7FF"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/opretbruger_txtView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="8dp"
        android:fontFamily="@font/open_sans_bold"
        android:text="Bliv mÃ¸deholder"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="@android:color/background_light"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/opretbruger_txtView2"
        android:layout_width="195dp"
        android:layout_height="24dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="8dp"
        android:fontFamily="@font/open_sans_semibold"
        android:text="Indtast dine oplysninger"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="@android:color/background_light"
        android:textSize="13sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/opretbruger_txtView1" />

</android.support.constraint.ConstraintLayout>
<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context=".aktiviteter.GiveFeedback2_akt"
tools:layout_editor_absoluteY="25dp">

<TextView
    android:id="@+id/feedback_nummer_txtView"
    android:layout_width="129dp"
    android:layout_height="62dp"
    android:layout_marginTop="84dp"
    android:fontFamily="@font/open_sans_bold"
    android:text="2 af 5"
    android:textAlignment="center"
    android:textColor="@color/colorGrøn"
    android:textSize="40sp"
    android:textStyle="bold"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent" />

<TextView
    android:id="@+id/feedback_spørgsmaal_txtView"
    android:layout_width="303dp"
    android:layout_height="76dp"
    android:layout_marginTop="72dp"
    android:text="Hvad syntes du om PowerPoint præsentationen?"
    android:textAlignment="center"
    android:textColor="@color/colorMørkegrå"
    android:textSize="22sp"
    android:textStyle="bold"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.506"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/feedback_nummer_txtView" />

    <LinearLayout
        android:layout_width="337dp"
        android:layout_height="72dp"
        android:layout_marginBottom="9dp"
        android:layout_marginTop="60dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/feedback_spørgsmaal_txtView"
        app:layout_constraintVertical_bias="0.0">

        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            app:srcCompat="@drawable/meget_sur" />

        <ImageView
            android:id="@+id/imageView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            app:srcCompat="@drawable/sur" />

        <ImageView
            android:id="@+id/imageView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            app:srcCompat="@drawable/tilfreds" />

        <ImageView
            android:id="@+id/imageView16"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            app:srcCompat="@drawable/meget_tilfreds" />

    </LinearLayout>

</android.support.constraint.ConstraintLayout>